        <td>{{ user.id }}</td>
        <td>{{ user.email }}</td>
        <td>{{ user.name }}</td>
        <td>{{ user.lastname }}</td>
        <td>{{ user.number }}</td>
         <td>{{ user.password }}</td>
   <td>
    {% for role in user.roles %}
        {{ role }}
        {% if not loop.last %}, {% endif %}
    {% endfor %}
</td>


        <!-- Ajoutez d'autres colonnes si nécessaire -->
    </tr>
{% else %}
