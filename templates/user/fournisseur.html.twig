                                <div class="table-responsive">
                                    <table id="zero_config" class="table table-striped table-bordered no-wrap">
                                      
                                        <thead>
                                        
                                            <tr>
                                                <th>ID</th>
                                                <th>Fournisseur's email</th>
                                                <th>FirstName</th>
                                                <th>LastName Date</th>
                                                <th>Number</th>
                                                <th>Password</th>
                                                <th>Role</th>
                                               
                                            </tr>
                                        </thead>
                                        <tbody>
                                           {% for user in fournisseurs %}
