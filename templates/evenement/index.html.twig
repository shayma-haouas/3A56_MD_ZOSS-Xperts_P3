                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">Events list</h4>
                                <a href="{{ path('app_evenement_new') }}" class="btn btn-primary rounded-pill" style="background-color: #0b3a58; border-color: #ADD8E6;">Create new</a>
                                <div class="table-responsive">
                                    <table id="zero_config" class="table table-striped table-bordered no-wrap">
                                        <thead>
                                            <tr>
                                                <th>ID</th>
                                                <th>Event's name</th>
                                                <th>Type</th>
                                                <th>Start Date</th>
                                                 <th>end date</th>
                                                <th>Description</th>
                                                <th> Number of participants</th>
                                                 <th> Place </th>
                                                <th> image</th>
                                                 <th> Sponsors</th>
                                                  <th> Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                  
                                           {% for e in evenements %}
                                            <tr>
                 <td>{{ e.id }}</td>
                <td>{{ e.nameevent }}</td>
                <td>{{ e.type }}</td>
                <td>{{ e.datedebut ? e.datedebut|date('Y-m-d') : '' }}</td>
                <td>{{ e.datefin ? e.datefin|date('Y-m-d') : '' }}</td>
                <td>{{ e.description }}</td>
                <td>{{ e.nbparticipant }}</td>
                <td>{{ e.lieu }}</td>
                <td>{{ e.image }}</td>
                <td>{{ e.sponsor.name }}</td>
                
                <td>
                  <a href="{{ path('app_evenement_show', {'id': e.id}) }}" class="btn btn-primary rounded-pill" style="background-color: #0b3a58; border-color: #ADD8E6; color: white;">Show</a>
<a href="{{ path('app_evenement_edit', {'id': e.id}) }}" class="btn btn-primary rounded-pill" style="background-color: #0b3a58; border-color: #ADD8E6; color: white;">Edit</a>
                </td>
                                                  
                  </tr>
          {% else %}
            <tr>
                <td colspan="10">no records found</td>
            </tr>
        {% endfor %}



